class UnaryOperatorSpacingUnitTestInc {
	public function one() {
		$a = [1, 2, 3];
		$i = 0;
		$b = $a[ ++$i ];
		$c = $a[ $i++ ];
		$d =  ~$b;
		$e = ! $d;
		$f = --$i;
		$g = &$i;
	}
}
